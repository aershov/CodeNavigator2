      <action id="codeNavigator.uml.diagramSettings"
              class="de.frag.umlplugin.actions.SettingsAction"
              text="Settings"
              description="Show diagram settings"
              icon="/de/frag/umlplugin/icons/settings.png">
      </action>
      <action id="codeNavigator.uml.diagramHelp"
              class="de.frag.umlplugin.actions.HelpAction"
              text="Plugin Help"
              description="Plugin Help"
              icon="/de/frag/umlplugin/icons/help.png">
      </action>
    </group>

    <!-- diagram node popup menu -->
    <group id="codeNavigator.uml.UMLDiagramNodePopupMenu" popup="true"/>
    <!-- sub menu for adding classes -->
    <group text="Add classes"
           description="Add classes with dependencies to current class"
           popup="true">
      <add-to-group anchor="first" group-id="codeNavigator.uml.UMLDiagramNodePopupMenu"/>
      <action id="codeNavigator.uml.addDependendClassesToDiagram"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.AddDependentClassesAction"
              text="with any dependencies"
              description="Use all dependency types to add classes">
      </action>
      <action id="codeNavigator.uml.addUsingClassesToDiagram"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.AddUsingClassesAction"
              text="that are using class"
              description="Add only using classes"
              icon="/de/frag/umlplugin/icons/using.png">
      </action>
      <action id="codeNavigator.uml.addUsedClassesToDiagram"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.AddUsedClassesAction"
              text="that are used by class"
              description="Add only used classes"
              icon="/de/frag/umlplugin/icons/used.png">
      </action>
      <action id="codeNavigator.uml.addExtendingClassesToDiagram"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.AddExtendingClassesAction"
              text="that are extending class"
              description="Add only extending classes"
              icon="/de/frag/umlplugin/icons/extending.png">
      </action>
      <action id="codeNavigator.uml.addExtendedClassesToDiagram"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.AddExtendedClassesAction"
              text="that are extended by class"
              description="Add only extended classes"
              icon="/de/frag/umlplugin/icons/extended.png">
      </action>
      <action id="codeNavigator.uml.addFieldClassesToDiagram"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.AddFieldClassesAction"
              text="using or used via fields"
              description="Add classes using or used via fields"
              icon="/de/frag/umlplugin/icons/field.png">
      </action>
    </group>
    <!-- sub menu for connecting all classes -->
    <group popup="true"
           text="Connect all classes"
           description="Connect all classes contained in diagram">
      <add-to-group anchor="last" group-id="codeNavigator.uml.UMLDiagramNodePopupMenu"/>
      <action id="codeNavigator.uml.connectClasses"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ConnectClassesAction"
              text="with any dependencies"
              description="Connect classes with any dependencies">
      </action>
      <action id="codeNavigator.uml.connectClassesUsed"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ConnectClassesUsedAction"
              text="that use each other"
              description="Connect classes that use each other"
              icon="/de/frag/umlplugin/icons/used.png">
      </action>
      <action id="codeNavigator.uml.connectClassesExtended"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ConnectClassesExtendedAction"
              text="that extend each other"
              description="Connect classes that extend each other"
              icon="/de/frag/umlplugin/icons/extended.png">
      </action>
      <action id="codeNavigator.uml.connectClassesField"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ConnectClassesFieldAction"
              text="using or used via fields"
              description="Add classes using or used via fields"
              icon="/de/frag/umlplugin/icons/field.png">
      </action>
    </group>
    <!-- sub menu for connecting single classes -->
    <group popup="true"
           text="Connect class to existing classes"
           description="Connects class to all classes already contained in diagram">
      <add-to-group anchor="last" group-id="codeNavigator.uml.UMLDiagramNodePopupMenu"/>
      <action id="codeNavigator.uml.connectSingleClass"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ConnectSingleClassAction"
              text="with any dependencies"
              description="Connect to classes with any dependencies">
      </action>
      <action id="codeNavigator.uml.connectSingleClassUsing"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ConnectSingleClassUsingAction"
              text="that are using class"
              description="Connect to classes that are using current class"
              icon="/de/frag/umlplugin/icons/using.png">
      </action>
      <action id="codeNavigator.uml.connectSingleClassUsed"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ConnectSingleClassUsedAction"
              text="that are used by class"
              description="Connect classes that are used by current class"
              icon="/de/frag/umlplugin/icons/used.png">
      </action>
      <action id="codeNavigator.uml.connectSingleClassExtending"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ConnectSingleClassExtendingAction"
              text="that are extending class"
              description="Connect classes that are extending current class"
              icon="/de/frag/umlplugin/icons/extending.png">
      </action>
      <action id="codeNavigator.uml.connectSingleClassExtended"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ConnectSingleClassExtendedAction"
              text="that are extended by class"
              description="Connect classes that are extended by current class"
              icon="/de/frag/umlplugin/icons/extended.png">
      </action>
      <action id="codeNavigator.uml.connectSingleClassesField"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ConnectSingleClassFieldAction"
              text="using or used via fields"
              description="Add classes using or used via fields"
              icon="/de/frag/umlplugin/icons/field.png">
      </action>
    </group>
    <!-- toggle visibility of compartments for a single class -->
    <group popup="false">
      <add-to-group anchor="last" group-id="codeNavigator.uml.UMLDiagramNodePopupMenu"/>
      <separator/>
    </group>
    <group text="Change visibility for current class"
           description="Change compartment visibility for current class"
           popup="true">
      <add-to-group anchor="last" group-id="codeNavigator.uml.UMLDiagramNodePopupMenu"/>
      <separator/>
      <action id="codeNavigator.uml.toggleFields"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ToggleFieldsAction"
              text="Toggle Fields"
              description="Toggle field compartment visibility for current class">
      </action>
      <action id="codeNavigator.uml.toggleMethods"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ToggleMethodsAction"
              text="Toggle Methods"
              description="Toggle method compartment visibility for current class">
      </action>
      <action id="codeNavigator.uml.toggleFieldsAndMethods"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ToggleFieldsAndMethodsAction"
              text="Toggle Fields and Methods"
              description="Toggle field and method compartment visibility for current class">
      </action>
      <action id="codeNavigator.uml.hideFieldsAndMethods"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.HideFieldsAndMethodsAction"
              text="Hide Fields and Methods"
              description="Hide field and method compartment visibility for current class">
      </action>
    </group>
    <!-- toggle visibility of compartments for a all classes -->
    <group text="Change visibility for all classes"
           description="Change compartment visibility for all classes"
           popup="true">
      <add-to-group anchor="last" group-id="codeNavigator.uml.UMLDiagramNodePopupMenu"/>
      <action id="codeNavigator.uml.toggleFieldsForAll"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ToggleFieldsForAllClassesAction"
              text="Toggle Fields"
              description="Toggle field compartment visibility for all classes">
      </action>
      <action id="codeNavigator.uml.toggleMethodsForAll"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ToggleMethodsForAllClassesAction"
              text="Toggle Methods"
              description="Toggle method compartment visibility for all classes">
      </action>
      <action id="codeNavigator.uml.toggleFieldsAndMethodsForAll"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.ToggleFieldsAndMethodsForAllClassesAction"
              text="Toggle Fields and Methods"
              description="Toggle field and method compartment visibility for all classes">
      </action>
      <action id="codeNavigator.uml.hideFieldsAndMethodsForAll"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.HideFieldsAndMethodsForAllClassesAction"
              text="Hide Fields and Methods"
              description="Hide field and method compartment visibility for all classes">
      </action>
    </group>
    <!-- remaining actions -->
    <group popup="false">
      <add-to-group anchor="last" group-id="codeNavigator.uml.UMLDiagramNodePopupMenu"/>
      <separator/>
    </group>
    <group popup="false">
      <add-to-group anchor="last" group-id="codeNavigator.uml.UMLDiagramNodePopupMenu"/>
      <action id="codeNavigator.uml.removeClassFromDiagram"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.RemoveClassAction"
              text="Remove class"
              description="Remove class"
              icon="/de/frag/umlplugin/icons/remove.png">
      </action>
      <action id="codeNavigator.uml.diagramClear"
              class="de.frag.umlplugin.uml.actions.nodepopupmenu.DiagramClearAction"
              text="Clear diagram"
              description="Clear diagram"
              icon="/de/frag/umlplugin/icons/clear.png">
      </action>
      <separator/>
      <action id="codeNavigator.uml.showDiagramClassInGraphicalNavigator"
              class="de.frag.umlplugin.codenavigator.actions.ShowInGraphicalNavigatorAction"
              text="Show class in graphical navigator"
              description="Show class in graphical navigator">
      </action>
    </group>


    <!-- diagram edge popup menu -->
    <group id="codeNavigator.uml.UMLDiagramEdgePopupMenu" popup="true">
      <action id="codeNavigator.uml.removeEdgeFromDiagram"
              class="de.frag.umlplugin.uml.actions.edgepopupmenu.RemoveEdgeAction"
              text="Remove dependency edge"
              description="Remove dependency edge"
              icon="/de/frag/umlplugin/icons/remove.png">
      </action>
      <action id="codeNavigator.uml.removeAllUsageEdgesFromDiagram"
              class="de.frag.umlplugin.uml.actions.edgepopupmenu.RemoveAllUsageEdgesAction"
              text="Remove all usage edges"
              description="Remove all usage edges">
      </action>
      <action id="codeNavigator.uml.removeAllCreateEdgesFromDiagram"
              class="de.frag.umlplugin.uml.actions.edgepopupmenu.RemoveAllCreateEdgesAction"
              text="Remove all create edges"
              description="Remove all create edges">
      </action>
    </group>
  </actions>
</idea-plugin>